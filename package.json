{
  "name": "hh-gallery",
  "version": "1.0.0",
  "description": "Twitter image gallery",
  "main": "app.js",
  "author": "Cory Cook",
  "license": "MIT",
  "scripts": {
    "start": "node ./dist/app.js",
    "watch":
      "concurrently --names babel,app \"babel src -d dist --watch\" \"nodemon --watch dist dist/app.js\"",
    "build":
      "babel src -d dist && cpy \"**/*.graphql\" \"../dist\" --cwd=src --parents",
    "prepush": "npm run build",
    "push": "bx cf push"
  },
  "dependencies": {
    "@gramps/gramps-express": "^0.1.5",
    "adaro": "^1.0.4",
    "ajv": "^5.3.0",
    "apollo-fetch": "^0.6.0",
    "apollo-server-express": "^1.2.0",
    "babel": "^6.23.0",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-plugin-inline-import": "^2.0.6",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-2": "^6.24.1",
    "big-integer": "^1.6.25",
    "body-parser": "^1.18.2",
    "cfenv": "^1.0.4",
    "cloudant": "^1.10.0",
    "concurrently": "^3.5.0",
    "dataloader": "^1.3.0",
    "dotenv": "^4.0.0",
    "eslint-config-prettier": "^2.7.0",
    "eslint-plugin-prettier": "^2.3.1",
    "express": "^4.16.2",
    "express-session": "^1.15.6",
    "graphql": "^0.11.7",
    "graphql-tools": "^2.7.2",
    "log4js": "^2.3.11",
    "passport": "^0.4.0",
    "passport-twitter": "^1.0.4",
    "prettier": "^1.7.4",
    "request": "^2.83.0",
    "request-promise": "^4.2.2",
    "socket.io": "^2.0.4",
    "twitter": "^1.7.1",
    "cpy-cli": "^1.0.1"
  },
  "devDependencies": {
    "eslint": "^4.10.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.8.0",
    "nodemon": "^1.12.1"
  }
}
